{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ES5", "ES6"],
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "downlevelIteration": true,
    "strictNullChecks": true
  }
}
